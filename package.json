{
  "name": "@sequencemedia/eslint-config-standard",
  "version": "0.0.8",
  "description": "Standard configuration for ESLint 9",
  "main": "./index.mjs",
  "type": "module",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "https://sequencemedia.net"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sequencemedia/eslint-config-standard.git"
  },
  "homepage": "https://github.com/sequencemedia/eslint-config-standard#readme",
  "bugs": {
    "url": "https://github.com/sequencemedia/eslint-config-standard/issues"
  },
  "scripts": {
