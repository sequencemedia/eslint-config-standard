    "lint",
    "policy",
    "quality",
    "simple",
    "standard",
    "standard style",
    "style",
    "style checker",
    "style linter",
    "verify"
  ],
  "dependencies": {
    "@stylistic/eslint-plugin": "2.6.0-beta.0",
    "@typescript-eslint/eslint-plugin": "8.0.0-alpha.34",
    "@typescript-eslint/utils": "8.0.0-alpha.34",
    "eslint-plugin-n": "^17.9.0",
    "globals": "^15.8.0",
    "typescript-eslint": "8.0.0-alpha.34"
